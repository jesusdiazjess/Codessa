#include <stdio.h>

int main() {
  // Declare and initialize a 2x3 matrix
  int matrix[2][3] = {{1, 2, 3}, {4, 5, 6}};

  // Print the elements of the matrix
  printf("Elements of the matrix: \n");
  for (int i = 0; i < 2; i++) {
    for (int j = 0; j < 3; j++) {
      printf("%d ", matrix[i][j]);
    }
    printf("\n");
  }

  // Modify the elements of the matrix
  for (int i = 0; i < 2; i++) {
    for (int j = 0; j < 3; j++) {
      matrix[i][j] *= 2;
    }
  }

  // Print the modified elements of the matrix
  printf("Modified elements of the matrix: \n");
  for (int i = 0; i < 2; i++) {
    for (int j = 0; j < 3; j++) {
      printf("%d ", matrix[i][j]);
    }
    printf("\n");
  }

  return 0;
}
